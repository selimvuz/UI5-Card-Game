{
  "_version": "1.59.0",
  "sap.app": {
    "id": "cardgame.cardgame",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.4",
      "toolsId": "5465c3f1-0595-4632-ae13-867aa3800004"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "services": {
        "ShellUIService": {
            "factoryName": "sap.ushell.ui5service.ShellUIService"
        }
    },
    "dependencies": {
      "minUI5Version": "1.129.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "cardgame.cardgame.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "cardgame.cardgame.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
            "pattern": "",
            "name": "main",
            "target": "MainView"
        },
        {
            "pattern": "game",
            "name": "game",
            "target": "GameView"
        },
        {
          "pattern": "scores",
          "name": "scores",
          "target": "ScoresView"
        }
      ],
      "targets": {
        "MainView": {
            "viewName": "MainView",
            "viewLevel": 1,
            "viewPath": "cardgame.cardgame.view",
            "viewType": "XML"
        },
        "GameView": {
            "viewName": "GameView",
            "viewLevel": 2,
            "viewPath": "cardgame.cardgame.view",
            "viewType": "XML"
        },
        "ScoresView": {
          "viewName": "ScoresView",
          "viewLevel": 3,
          "viewPath": "cardgame.cardgame.view",
          "viewType": "XML"
        }
      }
    },
    "rootView": {
      "viewName": "cardgame.cardgame.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
